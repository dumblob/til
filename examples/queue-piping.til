set q [queue]

range 1 | send $q

io.out [pop $q] "should be 0"
io.out [pop $q] "should be 1"

range 2 | send $q
receive $q | foreach x {
    io.out "Received $x"
    if ($x == 2) { break }
}

# TODO: is_empty $q
# TODO: lenght $q

range 3 | send $q
receive.no_wait $q | foreach x {
    io.out "Received (no_wait) $x"
}
receive.no_wait $q | foreach x {
    io.out "Received (no_wait, again) $x"
}
